<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper   
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="SendTestLog">

	<insert id="insertSendTestLog" parameterType="HashMap">
		INSERT INTO
			send_test_log (uId, cmpCode, ddid, data, response, sendDt)
		VALUES
			(#{uId}, #{cmpCode}, #{ddid}, #{data}, #{response}, #{sendDt});
	</insert>
	
	<insert id="getSendTestLog" parameterType="HashMap">
		SELECT *
		FROM send_test_log
		WHERE
			1=1;
	</insert>
	
</mapper>